{
    "name": "guanguans/music-php",
    "description": "music search and download",
    "keywords": [
        "music",
        "download-songs",
        "dl",
        "search"
    ],
    "support": {
        "issues": "https://github.com/guanguans/music-php/issues",
        "source": "https://github.com/guanguans/music-php"
    },
    "type": "library",
    "authors": [
        {
            "name": "琯琯",
            "email": "yzmguanguan@gmail.com"
        }
    ],
    "require": {
        "php": ">=7.0",
        "guzzlehttp/guzzle": "^6.3",
        "metowolf/meting": "^1.5",
        "ext-json": "*",
        "symfony/console": "^3.2|^4.0"
    },
    "require-dev": {
        "phpunit/phpunit": "~5 | ~7",
        "overtrue/phplint": "~1.1",
        "mockery/mockery": "~1.1",
        "friendsofphp/php-cs-fixer": "~2.1",
        "phpstan/phpstan": "~0.9"
    },
    "license": "MIT",
    "minimum-stability": "dev",
    "autoload": {
        "psr-4": {
            "Guanguans\\MusicPhp\\": "src"
        },
        "files": [
            "src/Helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Guanguans\\Tests\\": "tests/"
        }
    },
    "config": {
        "sort-packages": true,
        "preferred-install": "dist"
    },
    "bin": [
        "bin/music-php"
    ],
    "scripts": {
        "unit": "./vendor/bin/phpunit --coverage-text --color=always",
        "lint": "./vendor/bin/phplint --ansi",
        "fix": "./vendor/bin/php-cs-fixer fix --ansi",
        "stan": "./vendor/bin/phpstan analyse --ansi",
        "test": [
            "@unit",
            "@lint",
            "@fix",
            "@stan"
        ]
    },
    "scripts-descriptions": {
        "unit": "Run the phpunit tests.",
        "lint": "Run the phplint tests.",
        "fix": "Run the php-cs-fixer tests.",
        "stan": "Run the phpstan tests",
        "test": "Run all tests."
    }
}
